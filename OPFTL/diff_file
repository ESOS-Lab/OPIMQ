+  		prev_eidx = N_EPOCH -1 ;
+  } else {
+  		prev_eidx = eidx - 1;
+  }
+  
+  if (g_barrierContext.stream[sidx].epoch_list[prev_eidx].epoch_id == eid -1) {
+  		return g_barrierContext.stream[sidx].epoch_list[prev_eidx].state;
+  } else {
+  		return EPOCH_STATE_NONE;
+  }
